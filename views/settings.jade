extends ./layout.jade

block content
 .navbar
  .navbar-inner
    .left.sliding
      a(href="/").back.link
        i.icon.icon-back
        span Back
    .center.sliding Settings
 .pages.navbar-through
  .page(data-page="infinite-scroll")
    .page-content.infinite-scroll(data-distance="50")
      div(class='content-block-title' style='font-size:2.1em; color: blue') Dogpatch Explorers
      .list-block.center
        ul
          -for (i = 1; i <= 80; i++)
            li.item-content
              .item-inner
                div(class='item-title' style='margin:0 auto') 
                  img(src='http://lorempixel.com/600/600')