body#to-top.index
  //- Side nav
  nav.side-nav(role='navigation')
    ul.nav-side-nav
      li
        a.tooltip-side-nav(href='#section-1', title='', data-original-title='Services', data-placement='left')
      li
        a.tooltip-side-nav(href='#section-2', title='', data-original-title='Features', data-placement='left')
      li
